OCCI_ID			= <%= @storage.id %>
NAME          	= <%= @storage.title.inspect %>"
DESCRIPTION   	= <%= @storage.summary if @storage.summary %>"

<% if @storage.attributes!.org!.opennebula!.storage!.type %>
TYPE          	= <%= @storage.attributes!.org!.opennebula!.storage!.type.upcase %>
<% elsif @storage.attributes!.org!.opennebula!.storage!.size %>
TYPE          	= DATABLOCK
<% end %>

PERSISTENT    	= <%= @storage.attributes!.org!.opennebula!.storage!.persistent.upcase %>
DEV_PREFIX    	= <%= @storage.attributes!.org!.opennebula!.storage!.dev_prefix.downcase  %>
BUS           	= <%= @storage.attributes!.org!.opennebula!.storage!.bus.downcase %>

SIZE          	= <%= @storage.attributes!.org!.opennebula!.storage!.size %>

FSTYPE        	= <%= @storage.attributes!.org!.opennebula!.storage!.fstype %>