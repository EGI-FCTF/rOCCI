{
    "mixins":[
        {
            "term":"large",
            "scheme":"/occi/templates#",
            "title":"Large Instance",
            "attributes":{
                "occi":{
                    "compute":{
                        "architecture":{
                            "mutable":false,
                            "type":"string",
                            "default":"x64"
                        },
                        "cores":{
                            "mutable":false,
                            "type":"number",
                            "default":2
                        },
                        "speed":{
                            "mutable":false,
                            "type":"number",
                            "default":2
                        },
                        "memory":{
                            "mutable":false,
                            "type":"number",
                            "default":7.5
                        }
                    },
                    "storage":{
                        "size":{
                            "mutable":false,
                            "type":"number",
                            "default":850
                        }
                    }
                }
            }
        }
    ]
}