{
    "mixins":[
        {
            "term":"extra_large",
            "scheme":"/occi/templates#",
            "title":"Extra Large Instance",
            "attributes":{
                "occi":{
                    "compute":{
                        "architecture":{
                            "mutable":false,
                            "type":"string",
                            "default":"x64"
                        },
                        "cores":{
                            "mutable":false,
                            "type":"number",
                            "default":4
                        },
                        "speed":{
                            "mutable":false,
                            "type":"number",
                            "default":2
                        },
                        "memory":{
                            "mutable":false,
                            "type":"number",
                            "default":15
                        }
                    }
                }
            }
        }
    ]
}