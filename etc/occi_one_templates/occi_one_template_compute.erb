# for testing purposes
DISK = [ IMAGE = "Ubuntu Desktop"]
NIC  = [ NETWORK = "VMLAN"]
  
<% storageLinks.each do |storageLink| %>
DISK = [ IMAGE = "<%= storageLink.attributes["id"] if links != nil %>"]
<%end%>
   
<% networkLinks.each do |networkLink| %>
NIC = [ NETWORK = "<%= networkLink.attributes["id"] if links != nil%>"]
<%end%>

GRAPHICS = [type="vnc"]
  
RANK = FREECPU
  
FEATURES=[ acpi="no" ]

HAIZEA=	[	
			start 		=	"<%= attributes["occi.reservation.start"] if attributes["occi.reservation.start"]%>",
			leasetype 	= 	"<%= attributes["occi.reservation.leastype"] if attributes["occi.reservation.leastype"]%>",
			duration 	= 	"<%= attributes["occi.reservation.duration"] if attributes["occi.reservation.duration"]%>",
			preemptible	= 	"<%= attributes["occi.reservation.preemptible"] if attributes["occi.reservation.preemptible"]%>",
			strategy	= 	"<%= attributes["occi.reservation.strategy"] if attributes["occi.reservation.strategy"]%>"
		]	

CONTEXT = [ HOSTNAME = "<%= attributes["occi.compute.hostname"] if attributes["occi.compute.hostname"]%>" ]