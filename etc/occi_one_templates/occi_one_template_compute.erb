<% if attributes["occi.core.title"] %>
NAME = "<%= attributes["occi.core.title"]%>"
<%end%>
MEMORY = "<%= attributes["occi.compute.memory"] if attributes["occi.compute.memory"]%>"
ARCHITECTURE = "<%= attributes["occi.compute.architecture"] if attributes["occi.compute.architecture"]%>"
SPEED = "<%= attributes["occi.compute.speed"] if attributes["occi.compute.speed"]%>"
CORES = "<%= attributes["occi.compute.cores"] if attributes["occi.compute.cores"]%>"
CPU = "<%= attributes["occi.compute.cores"] if attributes["occi.compute.cores"]%>"

# for testing purposes
DISK = [ IMAGE = "Ubuntu Desktop"]
NIC  = [ NETWORK = "VMLAN"]
  
<% storageLinks.each do |storageLink| %>
DISK = [ IMAGE = "<%= storageLink.attributes["id"] if links != nil %>"]
<%end%>
   
<% networkLinks.each do |networkLink| %>
NIC = [ NETWORK = "<%= networkLink.attributes["id"] if links != nil%>"]
<%end%>

GRAPHICS = [type="vnc"]
  
RANK = FREECPU
  
FEATURES=[ acpi="no" ]